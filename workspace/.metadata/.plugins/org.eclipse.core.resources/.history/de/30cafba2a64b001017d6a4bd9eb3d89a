package clearminds.cuentas;

public class Cuentas {
	// encapsulados
	private String id;
	private String tipo = "A";
	private double saldo;

	// get y set
	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public double getSaldo() {
		return saldo;
	}

	public void setSaldo(double saldo) {
		this.saldo = saldo;
	}

	public String getId() {
		return id;
	}

}
